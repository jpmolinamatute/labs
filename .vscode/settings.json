{
    "tslint.configFile": "client/angular/tslint.json",
    "eslint.alwaysShowStatus": true,
    "eslint.options": {
        "configFile": "./package.json"
    },
    "eslint.nodePath": "node_modules",
    "files.watcherExclude": {
        ".git/objects/**": true,
        ".git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "server/Python/.venv": true,
        "server/Python/python": true
    },
    "javascript.validate.enable": false,
    "python.linting.flake8Enabled": false,
    "python.linting.pylintEnabled": true,
    "python.linting.enabled": true,
    "python.pythonPath": "server/Python/.venv/bin/python",
    "python.venvPath": "server/Python/.venv",
    "python.linting.pylintPath": "server/Python/.venv/bin/pylint"
}
